{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "*"
  ],
  "webviews": [
    "*"
  ],
  "permissions": [
    "window:allow-create",
    "webview:allow-create-webview",
    "path:allow-resolve",
    "path:allow-resolve-directory",
    "webview:allow-internal-toggle-devtools",
    "shell:allow-open",
    "event:allow-listen"
  ]
}
